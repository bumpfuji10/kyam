<div class="row">
  <%= form_with(model: @admin) do |f| %>

    <%= f.label :name, "name" %>
    <%= f.text_field :name %>

    <%= f.label :email, "email" %>
    <%= f.text_field :email %>

    <%= f.label :password, "password" %>
    <%= f.text_field :password %>

    <%= f.label :password_confirmation, "Password Confirmation" %>
    <%= f.text_field :passsword %>

    <%= f.submit "SIGN UP", class: "btn waves-effect waves-light" %>
  <% end %>
</div>
